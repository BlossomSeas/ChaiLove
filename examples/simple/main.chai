global logo
global x
global y

def load() {
	graphics.setBackgroundColor(54, 172, 248)
	logo = graphics.newImage("logo.png")
	x = 10.0f
	y = 10.0f
}

def draw() {
    graphics.print("Hello World!", 100, 100)
    graphics.draw(logo, x, y)
}

def update(delta) {
	x += delta * 60.0f
	y += delta * 30.0f
}
