/**
 * \brief A simple example of using ChiaLove.
 */
global logo
global x
global y

def load() {
	love.graphics.setBackgroundColor(54, 172, 248)
	logo = love.graphics.newImage("logo.png")
	x = 10.0f
	y = 10.0f
}

def draw() {
    love.graphics.print("Hello World!", 100, 100)
    love.graphics.draw(logo, x, y)
}

def update(delta) {
	x += delta * 60.0f
	y += delta * 30.0f
}
